{
    "manifest_version": 2,
    "name": "Element Rotator",
    "version": "1.0",
    "description": "Rotate elements on a webpage",
    "icons": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_icon": "icons/icon-48.png",
      "default_title": "Element Rotator",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["selector.js"]
      }
    ]
  }
  